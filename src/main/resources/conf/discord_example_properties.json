{
	"shop": {
		"site": "shop.example.com",
		"siteMaps": [
			"sitemap_products_1.xml"
		],
		"parameters": {
			"silent": false,
			"cycleTime": "30000",
			"stockCycleTime": "3600000",
			"connectionTimeout": "15000",
			"socketTimeout": "60000"
		}
	},
	"listeners": [
		{
			"name": "DiscordListener",
			"class": "com.glang.watch.discord.DiscordListener",
			"parameters": {
				"maxSleep": "10000",
				"maxEvents": "10",
				"rateLimit": "5",
				"maxLength": "25",
				"lowStock": "30",
				"ttsAlerts": false,
				"customAlert": "@everyone Store has been updated!",
				"alertFlags": "BACK_IN_STOCK,IN_STOCK",
				"endpoint": "https://discordapp.com/api/webhooks/{webhook.id}/{webhook.token}"
			}
		}
	]
}
